{
  "server": {
    "http_port": ":8081",
    "use_tls": false,
    "tls_cert": "../cert.pem",
    "tls_key": "../privkey.pem",
    "webrtc_port_min": 5000,
    "webrtc_port_max": 5999,
    "auth_token": "token",
    "host": "http://127.0.0.1:8080"
  },
  "streams": {
    "H264": {
      "on_demand": true,
      "disable_audio": true,
      "url": "rtsp://user:pass@ip:port"
    }
  }
}
